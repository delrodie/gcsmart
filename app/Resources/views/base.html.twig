<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
        <title>{% block title %}GC Smart 1.0{% endblock %}</title>

        {% block stylesheets %}
        <link href="{{ asset('ressources/vendor/admin/assets/css/bootstrap.min.css')}}" rel="stylesheet" type="text/css">
        <link href="{{ asset('ressources/vendor/admin/assets/css/icons.css')}}" rel="stylesheet" type="text/css">
        <link href="{{ asset('ressources/vendor/admin/assets/css/style.css')}}" rel="stylesheet" type="text/css">
      	<link href="{{ asset('ressources/vendor/assets/css/main.css')}}" rel="stylesheet" />
      	<!--[if lte IE 9]><link rel="stylesheet" href="{{ asset('assets/css/ie9.css')}}" /><![endif]-->
      	<noscript><link rel="stylesheet" href="{{ asset('ressources/vendor/assets/css/noscript.css')}}" /></noscript>
      	<link rel="stylesheet" type="text/css" href="{{ asset('ressources/css/gcsmart.css')}}">
        {% endblock %}

        <link rel="icon" type="image/x-icon" href="{{ asset('ressources/images/amoiries.png') }}" />
    </head>
    <body>
      <div id="wrapper">

      <!-- Header -->
      <header id="header">

        {% block body %}{% endblock %}

          <!-- Footer -->
      		<footer id="footer">
      			<p class="copyright">Copyright &copy; 2017 | GC SMART 1.0 | Tous droits Réservés | Conception:<a style target="_blank" href="http://nehikarts.com"><strong style="color: #31aae2;">NEHIK'Arts</strong></span></a></p>
      		</footer>

      	</div>

      		<!-- BG -->
      	<div id="bg"></div>
        {% block javascripts %}
        <script src="{{ asset('ressources/vendor/assets/js/jquery.min.js')}}"></script>
  			<script src="{{ asset('ressources/vendor/assets/js/skel.min.js')}}"></script>
  			<script src="{{ asset('ressources/vendor/assets/js/util.js')}}"></script>
  			<script src="{{ asset('ressources/vendor/assets/js/main.js')}}"></script>
        <script src="{{ asset('ressources/vendor/admin/assets/js/bootstrap.min.js')}}"></script>
        {% endblock %}
    </body>
</html>
